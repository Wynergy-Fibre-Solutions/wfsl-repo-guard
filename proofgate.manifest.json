{
  "name": "wfsl-repo-guard",
  "version": "0.1.0",
  "description": "WFSL Repository Guard. Emits deterministic structural repository state for ProofGate verification.",
  "owner": "Wynergy Fibre Solutions Ltd",
  "scope": "repository",
  "guardType": "structural",
  "tier": "opencore",
  "entrypoint": null,
  "artefacts": [
    "wfsl.repo.state.txt",
    "wfsl.repo.state.sha256",
    "wfsl.repo.verdict.json",
    "wfsl.repo.proofgate.bundle.json"
  ],
  "determinism": {
    "inputs": "git-working-tree",
    "outputs": "deterministic-text-and-json",
    "hash": "sha256"
  }
}
