{
  "name": "wfsl-repo-guard",
  "version": "0.1.0",
  "description": "WFSL Repo Admission Guard (RAG): deterministic repo hygiene and release sequencing enforcement with evidence emission.",
  "license": "MIT",
  "type": "module",
  "bin": {
    "wfsl-repo-guard": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "node -e \"try{require('node:fs').rmSync('dist',{recursive:true,force:true})}catch(e){}\"",
    "check:self": "node dist/cli.js check --root . --mode repo"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.9.3"
  }
}
